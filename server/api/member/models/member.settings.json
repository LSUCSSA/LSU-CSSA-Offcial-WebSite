{
  "connection": "default",
  "collectionName": "members",
  "info": {
    "name": "member"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "position": {
      "type": "enumeration",
      "enum": [
        "chair",
        "viceChair",
        "member"
      ],
      "default": "member",
      "required": true
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "member"
    },
    "department": {
      "type": "enumeration",
      "enum": [
        "Presidents",
        "IT",
        "Media",
        "PR",
        "EP",
        "Treasure",
        "Secretary"
      ],
      "required": true
    }
  }
}
