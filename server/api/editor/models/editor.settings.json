{
  "connection": "default",
  "collectionName": "articles",
  "info": {
    "name": "article"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "content": {
      "type": "richtext",
      "required": true
    },
    "publishDate": {
      "type": "datetime",
      "required": true
    },
    "publish_page": {
      "model": "publish-page",
      "via": "articles"
    }
  }
}
