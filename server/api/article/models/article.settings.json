{
  "connection": "default",
  "collectionName": "articles",
  "info": {
    "name": "Article"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "private": false,
      "required": true
    },
    "author": {
      "type": "string",
      "private": false,
      "required": true
    },
    "digest": {
      "type": "string"
    },
    "content": {
      "type": "text",
      "private": false,
      "required": true
    },
    "url": {
      "type": "string",
      "private": false,
      "required": true,
      "unique": false
    },
    "thumb_media_id": {
      "type": "string",
      "private": false,
      "required": true,
      "unique": false
    },
    "update_time": {
      "type": "datetime",
      "private": false,
      "required": true,
      "unique": false
    },
    "media_id": {
      "type": "string",
      "private": false,
      "required": true
    },
    "content_source_url": {
      "type": "string",
      "private": false,
      "required": true
    },
    "thumb_media": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    }
  }
}
